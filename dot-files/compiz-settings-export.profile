[place]
s0_mode = 1

[gnomecompat]
s0_run_command_screenshot_key = Disabled
s0_command_window_screenshot = gnome-screenshot -w
s0_run_command_window_screenshot_key = Disabled
s0_command_terminal = x-terminal-emulator
s0_run_command_terminal_key = <Control><Alt>t

[move]
s0_initiate_key = Disabled

[decor]
s0_active_shadow_color = #00000080
s0_inactive_shadow_color = #000000ff

[wall]
s0_thumb_highlight_gradient_shadow_color = #dfdfdfff
s0_arrow_base_color = #e6e6e6d9
s0_arrow_shadow_color = #dcdcdcd9

[animation]
s0_open_random_effects = animation:Curved Fold;animation:Horizontal Folds;animation:Dream;animation:Fade;animation:Glide 1;animation:Glide 2;animation:Magic Lamp;animation:Magic Lamp Wavy;animation:Sidekick;animation:Wave;animation:Zoom;
s0_close_random_effects = animation:Curved Fold;animation:Horizontal Folds;animation:Dream;animation:Fade;animation:Glide 1;animation:Glide 2;animation:Magic Lamp;animation:Magic Lamp Wavy;animation:Sidekick;animation:Wave;animation:Zoom;
s0_minimize_random_effects = animation:Curved Fold;animation:Horizontal Folds;animation:Dream;animation:Fade;animation:Glide 1;animation:Glide 2;animation:Magic Lamp;animation:Magic Lamp Wavy;animation:Sidekick;animation:Zoom;
s0_unminimize_effects = animation:Glide 2;
s0_unminimize_random_effects = animation:Curved Fold;animation:Horizontal Folds;animation:Dream;animation:Fade;animation:Glide 1;animation:Glide 2;animation:Magic Lamp;animation:Magic Lamp Wavy;animation:Sidekick;animation:Zoom;

[ezoom]
s0_zoom_box_outline_color = #2f2f4f9f
s0_zoom_box_fill_color = #2f2f2f4f

[scale]
s0_overlay_icon = 1
s0_initiate_all_edge = TopLeft
s0_initiate_all_key = <Shift><Super>w
s0_show_desktop = true

[unityshell]
s0_show_hud = Disabled
s0_execute_command = <Super>r
s0_show_desktop_key = <Super>d
s0_panel_first_menu = <Super>f
s0_window_right_maximize = Disabled
s0_window_left_maximize = Disabled
s0_panel_opacity = 0,957500
s0_show_launcher = <Super>a
s0_keyboard_focus = Disabled
s0_launcher_switcher_forward = Disabled
s0_launcher_switcher_prev = Disabled
s0_launcher_hide_mode = 1
s0_autohide_animation = 0
s0_edge_responsiveness = 5,228571
s0_backlight_mode = 4
s0_menus_fadein = 0
s0_menus_discovery_duration = 3
s0_alt_tab_next_window = <Super>Tab
s0_alt_tab_prev_window = <Control><Super>Tab
s0_disable_show_desktop = true

[expo]
s0_expo_edge = BottomLeft
s0_x_offset = 0
s0_y_offset = 0
s0_distance = 0,005000
s0_vp_saturation = 40,000000
s0_selected_color = #cc0000ff
s0_ground_color1 = #b3b3b3cc
s0_ground_color2 = #b3b3b300

[core]
s0_active_plugins = core;composite;opengl;compiztoolbox;copytex;decor;wall;imgpng;vpswitch;mousepoll;regex;commands;resize;unitymtgrabhandles;grid;place;gnomecompat;move;snap;animation;session;expo;fade;workarounds;ezoom;scale;unityshell;
s0_edge_delay = 112
s0_autoraise = false
s0_show_desktop_key = <Super>d
s0_show_desktop_edge = BottomRight
s0_hsize = 4
s0_number_of_desktops = 4

